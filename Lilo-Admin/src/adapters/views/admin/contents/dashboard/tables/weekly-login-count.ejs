<h4 class="text-secondary">Weekly Data</h4>
<div class="d-flex justify-content-center mt-4">
    <canvas
        id="loginsChart"
        class="w-100"
        style="max-width: 100%; height: 300px"
    ></canvas>
</div>
<table class="login-data-table" style="display: none">
    <thead>
        <tr>
            <th>Date</th>
            <th>Count In</th>
            <th>Count Out</th>
            <th>Earliest Time</th>
            <th>Latest Out</th>
            <th>Average Log In</th>
            <th>Avergae Log Out</th>
        </tr>
    </thead>
    <tbody>
        <% loginData.forEach(item => { %>
        <tr>
            <td><%= item.date %></td>
            <td><%= item.login_count %></td>
            <td><%= item.logout_count %></td>          
            <td><%= item.earliest_login_time %></td>
            <td><%= item.latest_out_time %></td>
            <td><%= item.avg_login_seconds %></td>
            <td><%= item.avg_logout_seconds %></td>
        </tr>
        <% }) %>
    </tbody>
</table>
